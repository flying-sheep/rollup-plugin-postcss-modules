{
    "extends": ["flying-sheep", "plugin:@typescript-eslint/recommended", "plugin:import/typescript"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": 0,
        "comma-dangle": [2, "only-multiline"]
    },
    "settings": {
        "import/resolver": {
            "typescript": {
                "alwaysTryTypes": true
            }
        }
    },
    "ignorePatterns": ["/test/fixtures/expected/*/out.js"]
}
