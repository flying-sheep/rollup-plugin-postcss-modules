{
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"target": "ES2020",
		"lib": ["ES2020"],
		//stronger types
		"strict": true,
		//more
		"removeComments": true,
		"sourceMap": true,
		"declaration": true,
